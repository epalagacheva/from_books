# user izbira +/- i vuvejda 2 chisla
print("1","for +,", "2", "for -")
user= int(input())


if user != 1 and user != 2:
    print("WRONG!")
else:
    a= int (input ("a="))
    b= int (input ("b="))

    if user == 1:
        op1=a+b
        print(f'a+b={op1}')
    elif user == 2:
        op2=a-b
        print(f'a-b={op2}')
    


